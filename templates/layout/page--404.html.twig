{% extends  "page.html.twig" %}
{% block content %}
	<div id="main" class="{{ container }} page-not-found">
		{% if breadcrumbs is not empty %}
			{{ breadcrumbs }}
		{% endif %}
		<div class="row text-center py-5">
			<div class="col-12 py-5">
				<h1>Oops, that link doesn’t work.</h1>
			</div>
			<div class="col-12">
				<h2>404 - Page Not Found</h2>
			</div>
			<div class="col-12 py-5">
				<hr>
			</div>
			<div class="col-12 pb-5">
				<p>The page might’ve been deleted or moved.</p>
			</div>
		</div>
	</div>
{% endblock %}
